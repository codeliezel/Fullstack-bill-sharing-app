{
  "name": "bsa",
  "version": "1.0.0",
  "main": "app.js",
  "author": "Funmi Olaiya",
  "license": "MIT",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "start:dev": "nodemon src/index.ts",
    "build": "npx tsc",
    "format": "prettier . --write",
    "migrate": "yarn sequelize:ts db:migrate",
    "seed": "yarn sequelize:ts db:seed:all",
    "create-db": "yarn sequelize:ts db:create",
    "drop-db": "yarn sequelize:ts db:drop",
    "sequelize:ts": "ts-node ./node_modules/.bin/sequelize",
    "start:db": "yarn run drop-db && yarn run create-db && yarn run migrate && yarn run seed"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "currency.js": "^2.0.4",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "prettier": "^3.1.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "sequelize": "^6.35.2",
    "sequelize-typescript": "^2.1.6",
    "supertest": "^6.3.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/prettier": "^3.0.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.2",
    "sequelize-cli": "6.3.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
